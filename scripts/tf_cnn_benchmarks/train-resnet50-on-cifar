nohup python tf_cnn_benchmarks.py --model=resnet50 --batch_size=128 --data_dir=/root/cifar10/ --optimizer=momentum --num_batches=100000 --local_parameter_device=cpu --use_nccl=True --train_dir=/data/resnet50_checkpoint --save_model_steps=800 --learning_rate=0.1 --num_epochs_per_decay=90 --learning_rate_decay_factor=0.1 > resnet_results.out &
nohup python backup_checkpoints.py -i /data/resnet50_checkpoint -o /data/resnet50_checkpoints -t 900 > resnet_checkpoint_results.out &
